
web:
	mkdir -p lib/web/public/js/vendor/ lib/web/public/js/vendor/bootstrap/
	cp lib/web/components/bootstrap/js/*.js lib/web/public/js/vendor/bootstrap/
	cp lib/web/components/jquery/jquery.min.js lib/web/public/js/vendor/jquery.min.js
	cp lib/web/components/d3/d3.min.js lib/web/public/js/vendor/d3.min.js
